{
  "name": "uvehavior",
  "version": "1.0.0",
  "description": "Behavioral Observation Software",
  "homepage": "https://github.com/Manolomon/uvehavior",
  "author": "Manolomon",
  "keywords": [
    "behavioral science",
    "scientific software",
    "angular",
    "angular 9",
    "electron",
    "sqlite3"
  ],
  "main": "main.js",
  "private": true,
  "snyk": true,
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "test": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "postinstall": "ngcc && npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:electron": "node postinstall",
    "format:fix": "pretty-quick --staged",
    "lint": "ng lint",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.0",
    "@angular-devkit/build-angular": "0.1001.7",
    "@angular-eslint/builder": "0.0.1-alpha.18",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular/cli": "10.1.7",
    "@angular/common": "10.1.6",
    "@angular/compiler": "10.1.6",
    "@angular/compiler-cli": "10.1.6",
    "@angular/core": "10.1.6",
    "@angular/forms": "10.1.6",
    "@angular/language-service": "10.1.6",
    "@angular/platform-browser": "10.1.6",
    "@angular/platform-browser-dynamic": "10.1.6",
    "@angular/router": "10.1.6",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.0.3",
    "@types/node": "12.12.6",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "chai": "4.2.0",
    "codelyzer": "6.0.2",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "devtron": "1.4.0",
    "electron": "22.3.25",
    "electron-builder": "22.8.1",
    "electron-reload": "1.5.0",
    "eslint": "7.10.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "6.0.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.1",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "8.1.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "rxjs": "6.6.3",
    "snyk": "^1.605.0",
    "spectron": "12.0.0",
    "ts-node": "9.0.0",
    "tslib": "2.0.1",
    "typescript": "4.0.7",
    "wait-on": "5.3.0",
    "webdriver-manager": "12.1.8",
    "zone.js": "0.10.3"
  },
  "dependencies": {
    "@akveo/ng2-completer": "9.0.1",
    "@angular/animations": "10.1.6",
    "@angular/cdk": "10.2.7",
    "@electron/remote": "1.0.4",
    "@fortawesome/fontawesome-free": "5.15.3",
    "@nebular/theme": "6.2.2",
    "angular-resize-event": "2.0.1",
    "bootstrap": "5.0.1",
    "echarts": "5.1.1",
    "hammerjs": "2.0.8",
    "ng2-completer": "9.0.1",
    "ng2-smart-table": "1.7.2",
    "ngx-echarts": "6.0.1",
    "resize-observer-polyfill": "1.5.1",
    "sqlite3": "5.0.2",
    "typeorm": "0.2.32"
  }
}
