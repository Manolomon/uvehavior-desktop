{
  "name": "uvehavior",
  "version": "0.9.0",
  "description": "Behavioral Observation Software",
  "homepage": "https://github.com/Manolomon/uvehavior",
  "author": "Manolomon",
  "keywords": [
    "behavioral science",
    "scientific software",
    "angular",
    "angular 9",
    "electron",
    "sqlite3"
  ],
  "main": "main.js",
  "private": true,
  "snyk": true,
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "test": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "postinstall": "ngcc && npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:electron": "node postinstall",
    "format:fix": "pretty-quick --staged",
    "lint": "ng lint",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix"
    }
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.1.0",
    "@angular-devkit/build-angular": "0.1000.5",
    "@angular-eslint/builder": "0.0.1-alpha.18",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular/cli": "9.1.7",
    "@angular/common": "9.1.6",
    "@angular/compiler": "9.1.6",
    "@angular/compiler-cli": "9.1.6",
    "@angular/core": "9.1.11",
    "@angular/forms": "9.1.6",
    "@angular/language-service": "9.1.6",
    "@angular/platform-browser": "9.1.6",
    "@angular/platform-browser-dynamic": "9.1.6",
    "@angular/router": "9.1.6",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "@schematics/angular": "9.1.4",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "7.0.2",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "chai": "4.2.0",
    "codelyzer": "5.2.2",
    "conventional-changelog-cli": "2.0.34",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "devtron": "1.4.0",
    "electron": "8.2.5",
    "electron-builder": "22.6.0",
    "electron-reload": "1.5.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.1",
    "husky": "4.2.5",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.0.4",
    "karma-coverage-istanbul-reporter": "3.0.0",
    "karma-electron": "6.3.0",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "mocha": "7.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "rxjs": "6.5.5",
    "snyk": "^1.346.0",
    "spectron": "10.0.1",
    "ts-node": "8.9.1",
    "tslib": "1.11.1",
    "typescript": "3.8.3",
    "wait-on": "4.0.2",
    "webdriver-manager": "12.1.7",
    "zone.js": "0.10.3"
  },
  "dependencies": {
    "@akveo/ng2-completer": "9.0.1",
    "@angular/animations": "9.1.11",
    "@angular/cdk": "9.2.4",
    "@fortawesome/fontawesome-free": "5.13.1",
    "@nebular/theme": "5.1.0",
    "angular-resize-event": "1.2.2",
    "bootstrap": "4.5.0",
    "echarts": "4.8.0",
    "hammerjs": "2.0.8",
    "ng-image-slider": "2.6.1",
    "ng2-smart-table": "1.6.0",
    "ngx-echarts": "5.0.0",
    "sqlite3": "4.2.0",
    "typeorm": "0.2.25"
  }
}
